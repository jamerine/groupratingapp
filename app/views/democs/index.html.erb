<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>single_rec</th>
    </tr>
  </thead>
  <tbody>
    <% @democs.each do |democ| %>
      <tr>
        <td><%= democ.single_rec %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a CSV File</h3>
  <%= form_tag import_democs_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
